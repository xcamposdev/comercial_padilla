<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="barcode_view_users_form_custom" model="ir.ui.view">
            <field name="name">res.user.form.view.custom</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[last()]" position="after">
                    <page string="Inventory Barcode">
                        <group>
                            <group>
                                <field name="x_pick"/>
                                <field name="x_pack"/>
                                <field name="x_out"/>
                            </group>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>