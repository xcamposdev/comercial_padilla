<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_template_search_form_view_stock_barcode_custom" model="ir.ui.view">
            <field name="name">product.template.search.stock_barcode.form.custom</field>
            <field name="model">product.template</field>
            <!-- <field name="mode">primary</field> -->
            <field name="inherit_id" ref="stock.product_template_search_form_view_stock"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='warehouse_id']" position="after">
                    <separator/>
                    <field name="x_product_packaging"/>
                </xpath>
            </field>
        </record>


        <record model="ir.ui.view" id="product_search_form_view_custom">
            <field name="name">Product Template Kanban Stock Custom</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_kanban_view"/>
            <field name="arch" type="xml">
                <xpath expr="//kanban" position="inside">
                    <field name="x_product_packaging"/>
                </xpath>
            </field>
        </record>
        
    </data>
</odoo>