<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_partner_form_padilla_custom" model="ir.ui.view">
            <field name="name">padilla_tss_partner.custom.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">

                <xpath expr="//notebook"  position="before">
                    <group>
                        <field name="x_is_tss"/>
                    </group>
                </xpath>
            </field>
        </record>



    </data>
</odoo>